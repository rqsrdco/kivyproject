    BoxLayout:
        orientation: "vertical"
        padding: dp(12)
        spacing: dp(39)

        MDTextFieldRound:
            id: text_field
            size_hint_y: None
            hint_text: "add new Role"
            normal_color: app.theme_cls.accent_color
            color_active: app.theme_cls.primary_light
